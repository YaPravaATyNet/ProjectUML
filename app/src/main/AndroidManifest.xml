        <activity android:name=".view.TargetList" />
        <activity android:name=".view.Unit_List" />
        <activity android:name=".view.Unit"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

